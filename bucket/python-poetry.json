{
    "version": "1.1.11",
    "homepage": "https://python-poetry.org/",
    "description": "Dependency Management for Python",
    "url": "https://raw.githubusercontent.com/python-poetry/poetry/master/install-poetry.py",
    "hash": "e8c7f91d5bbf4d0795919cd062eafbb350bec0c7579cb61d05c2112f224935a2",
    "license": "MIT",
    "depends": "python",
    "env_set": {
        "POETRY_HOME": "$dir"
    },
    "bin": "bin\\poetry.exe",
    "checkver": {
        "github": "https://github.com/python-poetry/poetry"
    },
    "installer": {
        "script": "$env:POETRY_HOME=$dir; python $dir/$fname -y"
    },
    "uninstaller": {
        "script": "python $dir/install-poetry.py --uninstall -y"
    }
}
